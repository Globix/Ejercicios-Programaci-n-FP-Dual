package ejercicios;

import java.util.Arrays;

import main.Main;

public class CuartoEjercicio {

	public static void main(String[] args) {
		final int tamañoMatriz = 10;
		int[] lista = new int[30];

		System.out.println("Bienvenido al ejercicio 3\n");

		lista = numerosAleatorios(numeroRangoMinimo, numeroRangoMaximo, lista);
		Arrays.sort(lista);
		imprimirLista(lista);

		System.out.println("Fin del ejercicio\n");
		Main.main(null);
	}
	
	public static void calcularMatrizUnaria(int[][] matriz){
		for (int i = 0; i < matriz.length; i++){
			for (int j = 0; j < matriz[i].length; j++){
				if (i == j){
					matriz[i][j] = 1;
				} else {
					matriz[i][j] = 0;
				}
			}
			
		}
	}

}
