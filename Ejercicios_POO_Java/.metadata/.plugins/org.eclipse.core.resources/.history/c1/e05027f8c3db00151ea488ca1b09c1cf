package ejercicios;

import java.util.concurrent.ThreadLocalRandom;;

public class SegundoEjercicio {

	public static void main(String[] args) {
		
		int[] lista = new int[25];
		int[] auxiliar = new int[4];
		
		for (int item:lista){
			item = ThreadLocalRandom.current().nextInt(25, 51);
			System.out.print(item + " ");
		}
		
		for (int item:lista){
			if (item%2 == 0){
				auxiliar[0]++;
			}
			if (item%2 == 1){
				auxiliar[1]++;
			}
			auxiliar[2] += item;
		}
		
		lista.toString();

	}

}
